            count = count.substring(6, count.length);
            if (count == 0) {
                if ($(all).is(":checked")) {
                    for (var i = 0; i < allregion.length; i++) {
                        if (!$(allregion[i]).is(":checked")) {
                            $(allregion[i]).prop("checked", true);
                        }
